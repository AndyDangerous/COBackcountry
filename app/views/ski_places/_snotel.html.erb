<h5>Nearby SNOTEL Readings</h5>
<h6><%= @ski_place.snotel_station.name %></h6>

<% @fu = @ski_place.snotel_station.daily %>

<table>
  <tr>
    <td>
      New Snow
    </td>
    <td>
      <%= "#{@fu.first[:snow_water_equivalent_in]}" %>
    </td>
  </tr>
  <tr>
    <td>
      New SWE
    </td>
    <td>
      <%= "#{@fu.first[:change_in_snow_water_equivalent_in]}" %>
    </td>
  </tr>
  <tr>
    <td>
      Snow Depth
    </td>
    <td>
      <%= "#{@fu.first[:snow_depth_in]}" %>
    </td>
  </tr>
</table>
